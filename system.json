{
  "name": "foundry-starforged",
  "title": "Ironsworn: Starforged",
  "description": "A WIP implementation of the Ironsworn: Starforged system. Based on the Ironsworn foundry module by Ben Straub.",
  "version": "0.2.4",
  "minimumCoreVersion": "0.7.5",
  "compatibleCoreVersion": "0.7.9",
  "templateVersion": 2,
  "author": "James Reeves",
  "esmodules": [
    "module/ironsworn.js"
  ],
  "styles": [
    "styles/ironsworn.css"
  ],
  "packs": [
    {
      "name": "ironsworn-tables",
      "label": "Ironsworn Tables",
      "system": "foundry-starforged",
      "path": "packs/tables.db",
      "entity": "RollTable"
    },
    {
      "name": "ironsworn-items",
      "label": "Ironsworn Moves",
      "system": "foundry-starforged",
      "path": "packs/items.db",
      "entity": "Item"
    },
    {
      "name": "ironsworn-assets",
      "label": "Ironsworn Assets",
      "system": "foundry-starforged",
      "path": "packs/assets.db",
      "entity": "Item"
    },
    {
      "name": "ironsworn-scenes",
      "label": "Ironsworn Maps",
      "system": "foundry-starforged",
      "path": "packs/scenes.db",
      "entity": "Scene"
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    },
    {
      "lang": "de",
      "name": "Deutsch",
      "path": "lang/de.json"
    },
    {
      "lang": "cn",
      "name": "中文",
      "path": "lang/cn.json"
    }
  ],
  "gridDistance": 5,
  "gridUnits": "ft",
  "primaryTokenAttribute": "health",
  "secondaryTokenAttribute": "power",
  "url": "https://github.com/weavejester/foundry-starforged",
  "manifest": "https://raw.githubusercontent.com/weavejester/foundry-starforged/main/system.json",
  "download": "http://github.com/weavejester/foundry-starforged/archive/0.2.4.zip",
  "license": "LICENSE.txt"
}
